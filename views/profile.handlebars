<div class="profileContainer">
    <h1>User Profile</h1>
    <p>Username: {{ userData.user_name }}</p>
    <p>Email: {{ userData.email }}</p>
    
    <h3>Spots Submitted: </h3>
    <div>
        <ul>
            {{#each userData.spots}}
            <li>
                {{!-- {{ this }} --}}
                <a href="/spots/{{this.id}}">{{ this.spot_name }} - {{ this.steps }}</a>
            </li>
            {{/each}}
        </ul>
    </div>
    
</div>